<div>
  <h1>Productos disponibles</h1>
  {{#each payload}}
    <p>{{id}}</p>
    <p>{{title}}</p>
    <p>{{category}}</p>
    <p>{{stock}}</p>
    <p>{{price}}</p>
    {{/each}}
 
    {{#if hasPrevPage}}
    <a href={{prevLink}}><button>Anterior</button></a>
    {{/if}}
     {{#if hasNextPage}}
    <a href={{nextLink}}><button>Siguiente</button></a>
    {{/if}}
   
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="js/index.js"></script>